    "shortcuts": [
        {
            "name": "Start Generating Smiles",
            "short_name": "Generate Smile",
            "description": "Jump directly to the AI smile generation tool",
            "url": "/#generator",
            "icons": [
                {
                    "src": "/images/shortcut-generator.png",
                    "sizes": "96x96"
                }
            ]
        },
        {
            "name": "View Tutorial",
            "short_name": "Tutorial",
            "description": "Learn how to use the AI Smile Generator",
            "url": "/#how-it-works",
            "icons": [
                {
                    "src": "/images/shortcut-tutorial.png",
                    "sizes": "96x96"
                }
            ]
        }
    ],
    "share_target": {
        "action": "/#generator",
        "method": "POST",
        "enctype": "multipart/form-data",
        "params": {
            "files": [
                {
                    "name": "image",
                    "accept": ["image/jpeg", "image/png", "image/webp"]
                }
            ]
        }
    },
    "file_handlers": [
        {
            "action": "/#generator",
            "accept": {
                "image/jpeg": [".jpg", ".jpeg"],
                "image/png": [".png"],
                "image/webp": [".webp"]
            }
        }
    ],
    "protocol_handlers": [
        {
            "protocol": "web+aismile",
            "url": "/#generator?url=%s"
        }
    ],
    "edge_side_panel": {
        "preferred_width": 400
    },
    "launch_handler": {
        "client_mode": "navigate-existing"
    }
} 