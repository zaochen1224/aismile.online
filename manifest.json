    "share_target": {
        "action": "/#generator",
        "method": "POST",
        "enctype": "multipart/form-data",
        "params": {
            "files": [
                {
                    "name": "image",
                    "accept": ["image/jpeg", "image/png", "image/webp"]
                }
            ]
        }
    },
    "file_handlers": [
        {
            "action": "/#generator",
            "accept": {
                "image/jpeg": [".jpg", ".jpeg"],
                "image/png": [".png"],
                "image/webp": [".webp"]
            }
        }
    ],
    "protocol_handlers": [
        {
            "protocol": "web+aismile",
            "url": "/#generator?url=%s"
        }
    ],
    "edge_side_panel": {
        "preferred_width": 400
    },
    "launch_handler": {
        "client_mode": "navigate-existing"
